<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox spacing="10.0" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.javafxusuarios.InputController"
      prefHeight="400.0" prefWidth="600.0">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="10.0"/>
    </padding>
    <HBox spacing="5.0">
        <Label text="Name"/>
        <TextField fx:id="name"/>
    </HBox>
    <HBox spacing="5.0">
        <Label text="Password"/>
        <PasswordField fx:id="pass1" />
    </HBox>
    <HBox spacing="5.0">
        <Label text="Password"/>
        <PasswordField fx:id="pass2" />
    </HBox>
    <Button fx:id="save" text="Guardar" onAction="#onButtonSaveClick"></Button>

</VBox>
